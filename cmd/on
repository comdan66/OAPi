require('./libs/node/Init')
  .title('PI Server')
  .path('config', ['cmd', 'config', 'on.js'])
  .path('pythonEntry', ['cmd', 'libs', 'python', 'Main.py'])
  .gola((load, done) =>
    load('on/Check')(_ =>
        load('on/Server')(_ =>
          load('on/Python')(_ =>
            load('on/Ready')(
              done)))))
